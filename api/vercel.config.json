{
  "version": 2,
  "builds": [
    {
      "src": "index.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.9",
        "handler": "index.handler",
        "maxLambdaSize": "15mb",
        "environment": {
          "PYTHONPATH": "/var/task:/var/task/api"
        }
      }
    }
  ]
}
